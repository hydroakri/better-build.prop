# Basic MagiskHide properties

# RootBeer, Microsoft
ro.build.tags=release-keys

# Other
ro.build.type=user
ro.debuggable=0
ro.secure=1

# optimize parameter
persist.sys.scrollingcache=3
profiler.force_disable_ulog=1
logcat.live=disable
ro.kernel.checkjni=0
ro.config.nocheckin=1
wifi.supplicant_scan_interval=300
pm.sleep_mode=2
